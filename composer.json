{
    "name": "sun-asterisk/laravel-security-checker",
    "description": "Laravel composer.lock vulnerabilities checker",
    "license": "MIT",
    "require": {
        "php": ">=7.1.3",
        "ext-xmlwriter": "*",
        "laravel/framework": "^5.0|^6.0|^7.0|^8.0",
        "sensiolabs/security-checker": "^6.0"
    },
    "require-dev": {
        "sun-asterisk/coding-standard": "^0.2.3"
    },
    "autoload": {
        "psr-4": {
            "SunAsterisk\\LaravelSecurityChecker\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SunAsterisk\\LaravelSecurityChecker\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
